    "axios": "^0.21.1",
    "cssnano": "^5.0.3",
    "dotenv": "^9.0.2",
    "html-minifier": "^4.0.0",
    "luxon": "^1.27.0",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-footnote": "^3.0.2",
    "postcss": "^8.3.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^8.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-simple-vars": "^6.0.3",
