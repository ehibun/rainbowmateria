  "repository": {
    "type": "git",
    "url": "https://github.com/ehibun/rainbowmateria"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
