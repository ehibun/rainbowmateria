    "axios": "^0.21.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-footnote": "^3.0.2",
    "postcss": "^8.2.2",
    "postcss-color-mix": "^1.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^14.0.0",
    "postcss-mixins": "^7.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^6.0.2",
